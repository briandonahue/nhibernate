<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="net.sf.hibernate.test.Holder">
		<id name="id" column="id_" type="string" length="32" unsaved-value="null">
			<generator class="uuid.hex" />
		</id>
		<property name="name" unique="true" not-null="true" length="36" />
		<list name="ones">
			<key column="holder" />
			<index column="i" />
			<one-to-many class="net.sf.hibernate.test.One" />
		</list>
		<array name="fooArray">
			<key column="holder1" />
			<index column="j1" />
			<one-to-many class="net.sf.hibernate.test.Foo" />
		</array>
		<set name="foos">
			<key column="holder2" />
			<one-to-many class="net.sf.hibernate.test.Foo" />
		</set>
	</class>
</hibernate-mapping>