<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"  
                   namespace="NHibernate.PostSharp.Proxies.Tests" 
                   assembly="NHibernate.PostSharp.Proxies.Tests"
                   >
  <class name="Customer">
    <id name="Id">
      <generator class="native"/>
    </id>
    <property name="Name"/>
    <set name="Addresses">
      <key column="Customer"/>
      <one-to-many class="Address"/>
    </set>
  </class>
</hibernate-mapping>
